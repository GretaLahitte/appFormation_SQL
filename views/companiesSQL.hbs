<div class="conteneur">
<h1>Express App : {{title}}</h1><br>
<select name="company" id="companies">
	<option value="0">Choisissez une compagnie</option>
	{{#each result}}
		<option value="{{this.id}}">{{this.name}}</option>
	{{/each}}
</select>
</div>



<script>
    var comp = document.getElementById('companies');
    comp.addEventListener('change', function(evt) {
       window.location = "/formCompanySQL/"+comp.value;
    });
 </script>
